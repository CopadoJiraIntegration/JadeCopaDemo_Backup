        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Review_Status__c</fullName>
        <description>Status of the review for particular Reviewer</description>
        <externalId>false</externalId>
        <label>Review Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>In-Progress</fullName>
                    <default>false</default>
                    <label>In-Progress</label>
                </value>
                <value>
                    <fullName>Approved</fullName>
                    <default>false</default>
                    <label>Approved</label>
                </value>
                <value>
                    <fullName>Rejected</fullName>
                    <default>false</default>
                    <label>Rejected</label>
                </value>
                <value>
                    <fullName>Change Requested</fullName>
                    <default>false</default>
                    <label>Change Requested</label>
                </value>
                <value>
                    <fullName>Re-Review</fullName>
                    <default>false</default>
                    <label>Re-Review</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
