        <fullName>Only_reviewer_can_update_the_status</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
 ISCHANGED( Review_Status__c ),
  Reviewer__c   &lt;&gt;  $User.Id 
)</errorConditionFormula>
